{
  "manifest_version": 2,
  "name": "Newt - A Better New Tab",
  "short_name": "Newt",
  "version": "8.8.0",
  "author": "Garrett Downs",
  "description": "A replacement for Google Chrome's 'new tab' page that lets you easily access your favorite bookmarks and more.",
  "icons": {
    "256": "images/icon-256.png",
    "128": "images/icon-128.png",
    "64": "images/icon-64.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "24": "images/icon-24.png",
    "16": "images/icon-16.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options/options.html",
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "bookmarks",
    "tabs",
    "sessions",
    "topSites",
    "chrome://favicon/*"
  ]
}